<div class="banner" style="background-image: url(https://res.cloudinary.com/daktzdqij/image/upload/v1702266582/0818_FMOVIES_BACKGROUND_jly0d3.jpg);">
  <div class="container">
    <h1>Movies List</h1>
    <%= link_to "Create new List", new_list_path, class: "btn btn-primary" %>
  </div>
</div>
<div class="background">
  <div class="row">
    <div class="column">
      <div class="cards">
        <% @lists.each do |list|  %>
          <%=link_to list.name, list_path(list)%>
        <% end %>
      </div>
    </div>
  </div>
</div>
